{
  // ID: jbikoelcekcnkdlnjcembcjjhnhljgop
  "name": "Nauta",
  "description": "Manage Nauta connection/disconnection (Only works from Cuba)",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "dist/index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting",
    "alarms",
    "nativeMessaging"
  ],
  "host_permissions": [
    "https://secure.etecsa.net:8443/*",
    "https://freegeoip.app/*"
  ]
}